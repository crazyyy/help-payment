{
  "extends"      : ["plugin:prettier/recommended"],
  "parser"       : "babel-eslint",
  "parserOptions": {
    "ecmaVersion" : 6,
    "sourceType"  : "module",
    "ecmaFeatures": {
      "jsx"                         : true,
      "experimentalObjectRestSpread": false
    }
  },
  "env"          : {
    "browser" : true,
    "node"    : true,
    "jquery"  : true,
    "commonjs": true,
    "es6"     : true
  }
}
